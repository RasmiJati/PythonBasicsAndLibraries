    "# while loop"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "91f5ddc9",
   "metadata": {},
   "source": [
    "program to display number from 1 to 200"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "210a95a4",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1\n",
      "2\n",
      "3\n",
      "4\n",
      "5\n"
     ]
    }
   ],
   "source": [
    "i = 1\n",
    "n = 5\n",
    "while i<=n:\n",
    "    print(i)\n",
    "    i=i+1"
   ]
  },
